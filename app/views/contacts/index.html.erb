<h1>Contact</h1>
<p>
  <div class="pull-left">
    <b><%=Contact.count %> data </b> Contact <br>
  </div>
  <div class="pull-right">
    <% if can? :create, Contact %>
    <%= link_to "New Contact", new_contact_path, remote: true, class: "btn btn-primary", id:'test_btn' %>
    <% end %>

  </div>
  <div class="new-product"></div>
</p>

<table class="table table-borderd table-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Phone Number</th>
    <th colspan="3">Actions</th>
  </tr>
  </thead>

  <tbody id="content-table" class="contact-index">
    <%= render "index"%>
  </tbody>
</table>

<div id="contact-modal" class="modal fade"></div>
